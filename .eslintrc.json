{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    // Add any ESLint rules you want to customize
    "prettier/prettier": ["error", { "endOfLine": "auto" }],
    "semi": ["error", "always"]
  }
}

